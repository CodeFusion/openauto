      </signal>
      
		
      <signal name="CameraOffSignal">
         
			
         <arg name="bucStatus" type="i"/>
         
			
         <arg name="dvdStatus" type="i"/>
         
			
         <arg name="tvStatus" type="i"/>
         
			
         <arg name="cpStatus" type="i"/>
         
			
         <arg name="aaStatus" type="i"/>
         
		
      </signal>
      
	
