         <arg direction="out" name="return_value" type="i"/>
         
		
      </method>
      
		
      <signal name="BLM_SettingInfo">
         
			
         <arg name="setting_type" type="i"/>
         
			
         <arg name="enabled" type="i"/>
         
			
         <arg name="currentstep" type="i"/>
         
			
         <arg name="min" type="i"/>
         
			
         <arg name="max" type="i"/>
         
		
